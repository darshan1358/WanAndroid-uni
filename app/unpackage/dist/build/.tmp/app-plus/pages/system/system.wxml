<view class="content"><view class="status-bar"></view><wuc-tab vue-id="1" tab-list="{{tabList}}" tabCur="{{TabCur}}" tab-class="text-center text-white bg-blue text-common" select-class="text-white text-xl" data-event-opts="{{[['^change',[['tabChange']]],['^updateTabCur',[['__set_sync',['$0','TabCur','$event'],['']]]],['^updateTabCur',[['__set_sync',['$0','TabCur','$event'],['']]]]]}}" bind:change="__e" bind:updateTabCur="__e" bind:__l="__l"></wuc-tab><swiper class="swiper" current="{{TabCur}}" duration="300" circular="{{true}}" indicator-color="rgba(255,255,255,0)" indicator-active-color="rgba(255,255,255,0)" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{tabList}}" wx:for-item="tabItem" wx:for-index="swiperIndex" wx:key="swiperIndex"><swiper-item class="swiper-item"><block wx:if="{{TabCur===0}}"><view><block wx:for="{{systemList}}" wx:for-item="systemItem" wx:for-index="systemIndex" wx:key="systemIndex"><view data-event-opts="{{[['tap',[['jumpSystemList',[0,'$0','$1'],[[['systemList','',systemIndex,'name']],[['systemList','',systemIndex,'children']]]]]]]}}" class="navigation" bindtap="__e"><view class="navigation-name">{{systemItem.name}}</view><block wx:for="{{systemItem.children}}" wx:for-item="systemDetail" wx:for-index="detailIndex" wx:key="detailIndex"><view data-event-opts="{{[['tap',[['jumpSystemList',[detailIndex,'$0','$1'],[[['systemList','',systemIndex,'name']],[['systemList','',systemIndex,'children']]]]]]]}}" class="detail-name" catchtap="__e">{{systemDetail.name}}</view></block></view></block></view></block><block wx:else><view><block wx:for="{{navigationList}}" wx:for-item="navigationItem" wx:for-index="navigationIndex" wx:key="navigationIndex"><view class="navigation"><view class="navigation-name">{{navigationItem.name}}</view><block wx:for="{{navigationItem.articles}}" wx:for-item="navigationDetail" wx:for-index="detailIndex" wx:key="detailIndex"><view data-event-opts="{{[['tap',[['jumpDetailPage',['$0'],[[['navigationList','',navigationIndex],['articles','',detailIndex]]]]]]]}}" class="detail-name" bindtap="__e">{{''+navigationDetail.title+''}}</view></block></view></block></view></block></swiper-item></block></swiper></view>